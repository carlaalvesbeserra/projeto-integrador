@model CadastroDePedidos
@{
    ViewData["Title"] = "Cadastro de Pedidos";
}
<h1>@ViewData["Title"]</h1>
<form method="POST">
<p>
    <label asp-for="Nome">Nome:</label>
    <input asp-for="Nome"/>
    </p>
<p>
        <label asp-for="Telefone">Telefone:</label>
        <input asp-for="Telefone"/>
    </p>
<p> <label asp-for="Data">Data para a entrega:</label>
        <input asp-for="Data"/>
    </p>
    <p> <label asp-for="Pedido">Pedido:</label>
        <input asp-for="Pedido"/>
    </p>
    <p> <label asp-for="Quantidade">Quantidade:</label>
        <input asp-for="Quantidade"/>
    </p>

<input type="submit" value="Cadastrar">
</form>
    <footer>
        <p><a href="Index">‚Üê</a>&nbsp;</p>
    </footer>